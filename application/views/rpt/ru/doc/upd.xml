					<СвЮЛУч НаимОрг="{$v->buyer->company_name}" ИННЮЛ="{$v->buyer->company_tax_id}" КПП="{$v->buyer->company_tax_id2}"/>
				</ИдСв>
				<Контакт Тлф="{$v->buyer->company_phone}"/>
			</СвПокуп>

		</СвСчФакт>
		<ТаблСчФакт>
		{loop name="v->rows"}
			<СведТов НомСтр="1" НаимТов="{$value->product_name}" ОКЕИ_Тов="796" КолТов="{$value->product_quantity}" ЦенаТов="{$value->product_price}" СтТовБезНДС="{$value->product_sum_vatless}" НалСт="{$value->product_vat_rate}" СтТовУчНал="{$value->product_sum_total}">
