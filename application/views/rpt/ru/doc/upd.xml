				<Контакт Тлф="{$v->seller->company_phone}"/>
			</СвПрод>
			<ГрузОт>
				<ОнЖе>он же</ОнЖе>
			</ГрузОт>
			<ГрузПолуч ОКПО="{$v->reciever->company_code}" СтруктПодр="Основной склад" КраткНазв="{$v->reciever->company_name}">
				<ИдСв>
					<СвЮЛУч НаимОрг="{$v->reciever->company_name}" ИННЮЛ="{$v->reciever->company_tax_id}" КПП="{$v->reciever->company_tax_id2}"/>
