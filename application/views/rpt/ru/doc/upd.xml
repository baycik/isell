				</Акциз>
				<СумНал>
					<СумНал>{$value->product_sum_vat}</СумНал>
				</СумНал>
				<СвТД КодПроисх="{$value->origin_code}" НомерТД="{$value->party_label}"/>
				<ДопСведТов ПрТовРаб="1" НаимЕдИзм="{$value->product_unit}" КрНаимСтрПр="{$value->origin_name}" КодВидТов="{$value->product_code}"/>
			</СведТов>
		{/loop}

			<ВсегоОпл СтТовБезНДСВсего="{$v->footer->vatless}" СтТовУчНалВсего="{$v->footer->total}">
