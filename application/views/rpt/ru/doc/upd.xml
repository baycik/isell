			</ГрузПолуч>
			<СвПРД НомерПРД="" ДатаПРД="{$v->sell_bill_reference}"/>
			<СвПокуп ОКПО="{$v->buyer->company_code}" СтруктПодр="Основной склад" КраткНазв="{$v->buyer->company_name}">
