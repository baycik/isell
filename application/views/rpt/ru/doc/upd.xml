				</ТранГруз>
			</СвПер>
		</СвПродПер>
		<Подписант ОблПолн="0" Статус="1" ОснПолн="Должностные обязанности">
			<ЮЛ ИННЮЛ="{$v->seller->company_tax_id}" НаимОрг="{$v->seller->company_code}" Должн="Генеральный директор">
				<ФИО Фамилия="{$v->seller->company_director}"/>
			</ЮЛ>
		</Подписант>
